<div class="dashboard-container">

<h1>dashboard</h1>
<h3><%= @workout_plan.name %></h3>
<table>
  <tr>
    <th>
      Day
    </th>
    <th>
      Name
    </th>
    <th>
      Edit
    </th>
    <th>
      View Sets
    </th>
  </tr>
  <% @workout.each do |w| %>
  <tr>
    <td>
      <%= w.day %>
    </td>
    <td>
      <%= w.name %>
    </td>
    <td>
      <%= link_to "Edit", edit_workout_path(w.id) %>
    </td>
    <td>
      <%= link_to "View sets", dashboard_workout_path(w.id) %>
    </td>
    </tr>
    <% end %>
</table>
  <%= link_to new_workout_plan_workout_path(@workout_plan) do %>
    New Workout
  <% end %>

</div>
